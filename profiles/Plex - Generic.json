{
    "description": "Plex - Generic Profile (Based on V1.0 Spec)",
    "notes": "Compatibility based on generalized rules. Assumes NO audio passthrough for HD formats. Specific clients may vary.",
    "supported_containers": [
        "matroska",
        "mkv",
        "mp4",
        "mov",
        "m4v"
    ],
    "unsupported_containers_strict": [
        "avi",
        "ts",
        "m2ts"
    ],
    "supported_video_codecs": [
        "h264",
        "hevc"
    ],
    "max_h264_level": 41,
    "supported_audio_codecs": [
        "aac",
        "ac3",
        "eac3"
    ],
    "unsupported_audio_codecs_strict": [
        "dca",
        "dts",
        "truehd",
        "mlpm",
        "pcm",
        "pcm_s16be",
        "pcm_s16le",
        "pcm_s24be",
        "pcm_s24le",
        "pcm_f32le",
        "wmapro",
        "wmav2",
        "alac"
    ],
    "supported_subtitle_codecs": [
        "subrip",
        "ssa",
        "ass"
    ],
    "unsupported_subtitle_formats": [
        "hdmv_pgs_subtitle",
        "dvd_subtitle"
    ]
}